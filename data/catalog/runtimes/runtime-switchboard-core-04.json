{
  "runtimes": [
    {
      "id": "nim",
      "displayName": "Nim",
      "description": "Nim programming language compiler.",
      "executableName": "nim.exe",
      "versionArguments": [
        "--version"
      ],
      "discovery": {
        "pathGlobs": [
          "%ProgramFiles%\\Nim*\\bin\\nim.exe",
          "%ProgramFiles(x86)%\\Nim*\\bin\\nim.exe",
          "%USERPROFILE%\\scoop\\apps\\nim*\\current\\nim.exe"
        ],
        "directoryGlobs": [
          "%ProgramFiles%\\Nim*",
          "%ProgramFiles(x86)%\\Nim*",
          "%USERPROFILE%\\scoop\\apps\\nim*"
        ],
        "whereHints": [
          "nim.exe"
        ]
      }
    },
    {
