{
  "runtimes": [
    {
      "id": "luajit",
      "displayName": "LuaJIT",
      "description": "LuaJIT just-in-time compiler builds.",
      "executableName": "luajit.exe",
      "versionArguments": [
        "-v"
      ],
      "discovery": {
        "pathGlobs": [
          "%ProgramFiles%\\LuaJIT*\\luajit.exe",
          "%ProgramFiles(x86)%\\LuaJIT*\\luajit.exe",
          "%USERPROFILE%\\scoop\\apps\\luajit*\\current\\luajit.exe"
        ],
        "directoryGlobs": [
          "%ProgramFiles%\\LuaJIT*",
          "%ProgramFiles(x86)%\\LuaJIT*",
          "%USERPROFILE%\\scoop\\apps\\luajit*"
        ],
        "whereHints": [
          "luajit.exe"
        ]
      }
    },
    {
      "id": "julia",
      "displayName": "Julia",
      "description": "Julia programming language distributions.",
      "executableName": "julia.exe",
      "versionArguments": [
        "--version"
      ],
      "discovery": {
        "pathGlobs": [
          "%ProgramFiles%\\Julia*\\bin\\julia.exe",
          "%LocalAppData%\\Programs\\Julia*\\bin\\julia.exe",
          "%USERPROFILE%\\scoop\\apps\\julia*\\current\\bin\\julia.exe"
