{
  "runtimes": [
    {
      "id": "python",
      "displayName": "Python",
      "description": "CPython distributions installed from python.org, Microsoft Store, and automation tooling.",
      "executableName": "python.exe",
      "versionArguments": [
        "--version"
      ],
      "desiredVersion": "3.12",
      "discovery": {
        "pathGlobs": [
          "%ProgramFiles%\\Python*\\python.exe",
          "%ProgramFiles(x86)%\\Python*\\python.exe",
          "%LocalAppData%\\Programs\\Python\\Python*\\python.exe",
          "C:\\Python*\\python.exe",
          "%ProgramFiles%\\WindowsApps\\PythonSoftwareFoundation.Python*\\python.exe"
        ],
        "directoryGlobs": [
          "%ProgramFiles%\\Python*",
          "%ProgramFiles(x86)%\\Python*",
          "%LocalAppData%\\Programs\\Python\\Python*",
          "C:\\Python*"
        ],
        "registryProbes": [
          {
            "key": "HKLM\\SOFTWARE\\Python\\PythonCore",
            "valueNames": [
              "InstallPath",
              "ExecutablePath"
            ],
            "valueKind": "directory",
            "appendExecutable": "python.exe",
            "includeSubkeys": true
          },
          {
            "key": "HKLM\\SOFTWARE\\WOW6432Node\\Python\\PythonCore",
            "valueNames": [
              "InstallPath",
              "ExecutablePath"
            ],
            "valueKind": "directory",
            "appendExecutable": "python.exe",
            "includeSubkeys": true
          }
        ],
        "whereHints": [
          "python.exe"
        ]
      }
    },
    {
      "id": "node",
      "displayName": "Node.js",
      "description": "Node.js runtimes from nodejs.org, package managers, and developer tools.",
      "executableName": "node.exe",
      "versionArguments": [
        "--version"
      ],
      "desiredVersion": "20",
      "discovery": {
        "pathGlobs": [
          "%ProgramFiles%\\nodejs\\node.exe",
          "%ProgramFiles(x86)%\\nodejs\\node.exe",
          "%USERPROFILE%\\scoop\\apps\\nodejs*\\current\\node.exe",
          "%ProgramFiles%\\WindowsApps\\*nodejs*\\node.exe"
        ],
        "directoryGlobs": [
          "%ProgramFiles%\\nodejs",
          "%ProgramFiles(x86)%\\nodejs",
          "%USERPROFILE%\\scoop\\apps\\nodejs*",
          "%ChocolateyInstall%\\lib\\nodejs*\\tools"
        ],
        "registryProbes": [
          {
            "key": "HKLM\\SOFTWARE\\Node.js",
            "valueNames": [
              "InstallPath"
            ],
            "valueKind": "directory",
            "appendExecutable": "node.exe",
            "includeSubkeys": true
          },
          {
            "key": "HKLM\\SOFTWARE\\WOW6432Node\\Node.js",
            "valueNames": [
              "InstallPath"
            ],
            "valueKind": "directory",
            "appendExecutable": "node.exe",
            "includeSubkeys": true
          }
        ],
        "whereHints": [
          "node.exe"
        ]
      }
    },
    {
      "id": "jdk",
      "displayName": "Java",
      "description": "Oracle, Microsoft, and OpenJDK distributions that expose java.exe on PATH.",
      "executableName": "java.exe",
      "versionArguments": [
        "-version"
      ],
      "desiredVersion": "21",
      "discovery": {
        "pathGlobs": [
          "%ProgramFiles%\\Java\\*\\bin\\java.exe",
          "%ProgramFiles(x86)%\\Java\\*\\bin\\java.exe",
          "%ProgramFiles%\\Microsoft\\jdk*\\bin\\java.exe",
          "%ProgramFiles%\\Zulu\\*\\bin\\java.exe",
          "%ProgramFiles%\\AdoptOpenJDK\\*\\bin\\java.exe"
        ],
        "directoryGlobs": [
          "%ProgramFiles%\\Java*",
          "%ProgramFiles(x86)%\\Java*",
          "%ProgramFiles%\\Microsoft\\jdk*",
          "%ProgramFiles%\\Zulu*",
          "%ProgramFiles%\\AdoptOpenJDK*"
        ],
        "registryProbes": [
          {
            "key": "HKLM\\SOFTWARE\\JavaSoft\\Java Development Kit",
            "valueNames": [
              "JavaHome"
            ],
            "valueKind": "directory",
            "appendExecutable": "bin\\java.exe",
            "includeSubkeys": true
          },
          {
            "key": "HKLM\\SOFTWARE\\JavaSoft\\Java Runtime Environment",
            "valueNames": [
              "JavaHome"
            ],
            "valueKind": "directory",
            "appendExecutable": "bin\\java.exe",
            "includeSubkeys": true
          },
          {
            "key": "HKLM\\SOFTWARE\\WOW6432Node\\JavaSoft\\Java Development Kit",
            "valueNames": [
              "JavaHome"
            ],
            "valueKind": "directory",
            "appendExecutable": "bin\\java.exe",
            "includeSubkeys": true
          },
          {
            "key": "HKLM\\SOFTWARE\\WOW6432Node\\JavaSoft\\Java Runtime Environment",
            "valueNames": [
              "JavaHome"
            ],
            "valueKind": "directory",
            "appendExecutable": "bin\\java.exe",
            "includeSubkeys": true
          }
        ],
        "whereHints": [
          "java.exe"
        ]
      }
    },
    {
      "id": "dotnet",
      "displayName": ".NET",
      "description": ".NET SDK/hosting bundles that surface dotnet.exe for CLI switching.",
      "executableName": "dotnet.exe",
      "versionArguments": [
        "--version"
      ],
      "desiredVersion": "8",
      "discovery": {
        "pathGlobs": [
          "%ProgramFiles%\\dotnet\\dotnet.exe",
          "%ProgramFiles(x86)%\\dotnet\\dotnet.exe",
          "%ProgramFiles%\\Microsoft Visual Studio*\\dotnet\\dotnet.exe"
        ],
        "directoryGlobs": [
          "%ProgramFiles%\\dotnet",
          "%ProgramFiles(x86)%\\dotnet",
          "%ProgramFiles%\\Microsoft Visual Studio*\\dotnet"
        ],
        "registryProbes": [
          {
            "key": "HKLM\\SOFTWARE\\dotnet\\Setup\\InstalledVersions\\x64",
            "valueNames": [
              "InstallLocation"
            ],
            "valueKind": "directory",
            "appendExecutable": "dotnet.exe",
            "includeSubkeys": false
          },
          {
            "key": "HKLM\\SOFTWARE\\dotnet\\Setup\\InstalledVersions\\x86",
            "valueNames": [
              "InstallLocation"
            ],
            "valueKind": "directory",
            "appendExecutable": "dotnet.exe",
            "includeSubkeys": false
          }
        ],
        "whereHints": [
          "dotnet.exe"
        ]
      }
    }
  ]
}
