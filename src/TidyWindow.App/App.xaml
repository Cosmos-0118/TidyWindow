<Application x:Class="TidyWindow.App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="clr-namespace:TidyWindow.App.Converters"
             xmlns:local="clr-namespace:TidyWindow.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/TidyWindow;component/Resources/RegistryOptimizer.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/TidyWindow;component/Resources/ScrollBars.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/TidyWindow;component/Resources/CheckBoxes.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/TidyWindow;component/Resources/ComboBoxes.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <converters:InverseBooleanConverter x:Key="InverseBooleanConverter"/>
            <converters:BooleanToYesNoConverter x:Key="BooleanToYesNoConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
